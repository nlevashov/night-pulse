{
  "expo": {
    "name": "Night Pulse",
    "slug": "night-pulse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nightpulse",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nlevashov.nightpulse",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.587826292807-u0p69ioudogh954vfdfq4ajoge8g77tv"
            ]
          }
        ]
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ],
      "expo-secure-store",
      [
        "expo-background-fetch",
        {
          "minimumInterval": 900
        }
      ],
      ["@kingstinct/react-native-healthkit", {
        "NSHealthShareUsageDescription": "Night Pulse needs access to your sleep, heart rate, step count, and workout data to generate daily reports for your coach and detect when you've woken up.",
        "NSHealthUpdateUsageDescription": "Night Pulse does not write any data to Apple Health.",
        "background": true
      }],
      "@react-native-community/datetimepicker",
      "react-native-bottom-tabs"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eb8c46b0-08ca-4da1-a8f2-514931788f26"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "nlevashov",
    "android": {
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "package": "com.nlevashov.nightpulse"
    }
  }
}
